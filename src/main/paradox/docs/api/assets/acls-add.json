        "realm": "realm",
        "group": "some-group"
      }
    },
    {
      "permissions": [
        "acls:read",
        "acls:write",
        "acls:create"
      ],
      "identity": {
        "@id": "https://nexus.example.com/v1/realms/realm/users/user",
        "@type": "UserRef",
        "realm": "realm",
        "sub": "alice"
      }
    }
  ]
}