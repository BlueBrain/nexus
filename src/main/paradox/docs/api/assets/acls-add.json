{
  "acl": [
    {
      "permissions": [
        "read"
      ],
      "identity": {
        "@id": "https://nexus.example.com/v1/realms/realm",
        "@type": "AuthenticatedRef",
        "realm": "realm"
      }
    },
    {
      "permissions": [
        "read",
        "write"
      ],
      "identity": {
        "@id": "https://nexus.example.com/v1/realm/groups/some-group",
