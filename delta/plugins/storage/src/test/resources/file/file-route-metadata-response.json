{
  "@context" : [
    "https://bluebrain.github.io/nexus/contexts/files.json",
    "https://bluebrain.github.io/nexus/contexts/metadata.json"
  ],
  "@id" : "{{id}}",
  "@type" : "File",
  "_constrainedBy" : "https://bluebrain.github.io/nexus/schemas/files.json",
  "_createdAt" : "1970-01-01T00:00:00Z",
  "_createdBy" : "{{createdBy}}",
  "_storage" : {
    "@id": "{{storage}}",
    "@type": "{{storageType}}",
    "_rev": {{storageRev}}
  },
  "_bytes": {{bytes}},
  "_digest": {
    "_value": "{{digest}}",
    "_algorithm": "{{algorithm}}"
  },
  "_path": "{{path}}",
  "_filename": "{{filename}}",
  "_origin": "{{origin}}",
  "_mediaType": "{{mediaType}}",
  "_deprecated" : {{deprecated}},
  "_incoming" : "http://localhost/v1/files/{{project}}/{{label}}/incoming",
  "_outgoing" : "http://localhost/v1/files/{{project}}/{{label}}/outgoing",
  "_project" : "http://localhost/v1/projects/{{project}}",
  "_rev" : {{rev}},
  "_self" : "http://localhost/v1/files/{{project}}/{{label}}",
