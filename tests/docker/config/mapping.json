{
  "dynamic_templates": [
    {
      "id_labels": {
        "match_mapping_type": "string",
        "match": "idLabel",
        "mapping": {
          "type": "keyword"
        }
      }
    },
    {
      "labels": {
        "match_pattern": "regex",
        "match": "^(label|name)$",
        "mapping": {
          "type": "text",
          "analyzer": "nexus",
          "search_analyzer": "search_nexus",
          "fields": {
            "keyword": {
              "type": "keyword"
            }
          }
        }
      }
    },
    {
      "strings": {
        "match_mapping_type": "string",
        "mapping": {
          "type": "text",
          "analyzer": "nexus",
          "search_analyzer": "search_nexus",
          "fields": {
            "keyword": {
              "type": "keyword"
            }
          }
        }
      }
    }
  ],
  "properties": {
    "brainRegion": {
      "type": "object"
    },
    "description": {
      "type": "text",
      "analyzer": "nexus",
      "search_analyzer": "search_nexus"
    },
    "distribution": {
      "properties": {
        "contentSize": {
          "type": "long"
        }
      }
    },
    "coordinatesInBrainAtlas": {
      "properties": {
        "valueX": { "type": "float" },
        "valueY": { "type": "float" },
        "valueZ": { "type": "float" }
      }
    },
    "source": {
      "properties": {
        "title": { "type": "text" },
        "identifier": {
          "properties": {
            "value": { "type": "text" },
            "propertyID": { "type": "text" }
          }
        }
      }
    },
    "subject": {
      "properties": {
        "weight": {
          "properties": {
            "value": { "type": "double" },
            "minValue": { "type": "double" },
            "maxValue": { "type": "double" }
          }
        },
        "age": {
          "properties": {
            "value": { "type": "double" },
            "minValue": { "type": "double" },
            "maxValue": { "type": "double" }
          }
        }
      }
    },
    "generation": {
      "properties": {
        "startedAt": { "type": "date" },
        "endedAt": { "type": "date" }
      }
    },
    "createdAt": {
      "type": "date"
    },
    "updatedAt": {
      "type": "date"
    },
    "startedAt": {
      "type": "date"
    },
    "endedAt": {
      "type": "date"
    },
    "series": {
      "type": "nested",
      "properties": {
        "value": { "type": "double" }
      }
    },
    "neuronDensity": {
      "properties": {
        "value": { "type": "double" }
      }
    },
    "layerThickness": {
      "properties": {
        "value": { "type": "double" }
      }
    },
    "boutonDensity": {
      "properties": {
        "value": { "type": "double" }
      }
    },
    "curated": {
      "type": "boolean"
    },
    "preSynapticPathway": { "type": "nested" },
    "postSynapticPathway": { "type": "nested" },
    "morphologyFeature": {
      "type": "nested",
      "properties": {
        "value": { "type": "double" }
      }
    }
  }
}