{
  "@context": [
    "https://bluebrain.github.io/nexus/contexts/storages.json",
    "https://bluebrain.github.io/nexus/contexts/metadata.json"
  ],
  "@id": "https://bluebrain.github.io/nexus/vocabulary/{{id}}",
  "@type": [
    "Storage",
    "DiskStorage"
  ],
  "default": false,
  "maxFileSize" : {{maxFileSize}},
  "volume": "/default-volume",
  "readPermission": "{{read}}",
  "writePermission": "{{write}}",
  "_algorithm": "SHA-256",
  "_incoming": "{{self}}/incoming",
  "_outgoing": "{{self}}/outgoing",
  "_self": "{{self}}",
  "_constrainedBy": "https://bluebrain.github.io/nexus/schemas/storages.json",
  "_project": "{{project}}",
  "_rev": 1,
  "_deprecated": false,
  "_createdBy": "{{deltaUri}}/realms/{{realm}}/users/{{user}}",
  "_updatedBy": "{{deltaUri}}/realms/{{realm}}/users/{{user}}"
}