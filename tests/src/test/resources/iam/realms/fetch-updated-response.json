{
  "@context": [
    "https://bluebrain.github.io/nexus/contexts/realms.json",
    "https://bluebrain.github.io/nexus/contexts/metadata.json"
  ],
  "@id": "{{deltaUri}}/realms/{{label}}",
  "@type": "Realm",
  "logo": "http://nexus.example.com/logo-updated.png",
  "name": "Nexus Test Updated",
  "openIdConfig": "{{realm}}/.well-known/openid-configuration",
  "_label": "{{label}}",
  "_grantTypes": [
    "urn:openid:params:grant-type:ciba",
    "password",
    "clientCredentials",
    "refreshToken",
    "authorizationCode",
    "urn:ietf:params:oauth:grant-type:device_code",
    "implicit"
  ],
  "_issuer": "{{realm}}",
  "_authorizationEndpoint": "{{realm}}/protocol/openid-connect/auth",
  "_tokenEndpoint": "{{realm}}/protocol/openid-connect/token",
  "_revocationEndpoint": "{{realm}}/protocol/openid-connect/revoke",
  "_userInfoEndpoint": "{{realm}}/protocol/openid-connect/userinfo",
  "_endSessionEndpoint": "{{realm}}/protocol/openid-connect/logout",
  "_rev": {{rev}},
  "_deprecated": false,
  "_constrainedBy": "https://bluebrain.github.io/nexus/schemas/realms.json",
  "_self": "{{deltaUri}}/realms/{{label}}"
}